{
  "name": "fastify-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Fastify Typescript Boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint . --ext .ts",
    "lint:watch": "nodemon --watch src -e ts --exec npm run lint",
    "build": "npx tsc",
    "dev:start": "npm-run-all lint clean build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "test": "NODE_ENV=test npx jest --verbose --runInBand",
    "start": "node ./dist/index.js",
    "pgsql:migrate": "node ./dist/Tools/Migration/index.js"
  },
  "keywords": [
    "Fastify",
    "Typescript"
  ],
  "author": "Sokun Chorn",
  "license": "ISC",
  "dependencies": {
    "fastify": "^4.10.2",
    "@fastify/helmet": "^10.1.0",
    "fastify-metrics": "^10.0.0",
    "fastify-plugin": "^4.4.0",
    "@fastify/swagger": "^8.2.0",
    "@autotelic/fastify-opentelemetry": "^0.17.1",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@opentelemetry/instrumentation-http": "^0.34.0",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/instrumentation": "^0.34.0",
    "massive": "^6.10.2",
    "nconf": "^0.12.0",
    "pino": "^8.7.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/massive": "^5.4.4",
    "@types/nconf": "^0.10.3",
    "@types/node": "^18.11.10",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^29.3.1",
    "json-schema-to-typescript": "^11.0.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^9.1.1",
    "postgres-migrations": "^5.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
