{
  "name": "fastify-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Fastify Typescript Boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint . --ext .ts",
    "lint:watch": "nodemon --watch src -e ts --exec npm run lint",
    "build": "npx tsc",
    "dev:start": "npm-run-all lint clean build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "test": "NODE_ENV=test npx jest --verbose --runInBand",
    "start": "node ./dist/index.js",
    "pgsql:migrate": "node ./dist/Tools/Migration/index.js"
  },
  "keywords": [
    "Fastify",
    "Typescript"
  ],
  "author": "Sokun Chorn",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-helmet": "^4.0.2",
    "fastify-metrics": "^6.0.1",
    "fastify-plugin": "^2.0.0",
    "fastify-swagger": "^3.1.1",
    "massive": "^6.5.0",
    "nconf": "^0.10.0",
    "pino": "^6.3.2",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/massive": "^5.4.3",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.14.41",
    "@types/pino": "^6.3.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "jest": "^26.6.3",
    "json-schema-to-typescript": "^9.1.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.0",
    "pino-pretty": "^4.7.1",
    "postgres-migrations": "^4.0.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^8.10.0",
    "typescript": "^3.9.9"
  }
}
