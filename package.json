{
  "name": "fastify-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Fastify Typescript Boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint . --ext .ts",
    "lint:watch": "nodemon --watch src -e ts --exec npm run lint",
    "build": "npx tsc",
    "dev:start": "npm-run-all lint clean build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "test": "NODE_ENV=test npx jest --verbose --runInBand",
    "start": "node ./dist/index.js",
    "pgsql:migrate": "node ./dist/Tools/Migration/index.js"
  },
  "keywords": [
    "Fastify",
    "Typescript"
  ],
  "author": "Sokun Chorn",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.29.0",
    "@fastify/helmet": "^8.1.0",
    "fastify-metrics": "^8.0.0",
    "fastify-plugin": "^3.0.1",
    "@fastify/swagger": "^6.1.0",
    "@autotelic/fastify-opentelemetry": "^0.16.0",
    "@opentelemetry/api": "^1.1.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@opentelemetry/instrumentation-http": "^0.29.2",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/instrumentation": "^0.29.2",
    "massive": "^6.10.2",
    "nconf": "^0.12.0",
    "pino": "^8.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/massive": "^5.4.4",
    "@types/nconf": "^0.10.2",
    "@types/node": "^17.0.40",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "jest": "^28.1.1",
    "json-schema-to-typescript": "^10.1.5",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^8.0.0",
    "postgres-migrations": "^5.3.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
